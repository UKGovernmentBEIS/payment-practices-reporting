GET         /assets/*file                                            controllers.Assets.at(path="/public", file)

GET         /                                                        controllers.Default.redirect(to = controllers.routes.HomeController.index().url)

GET         /wip                                                     controllers.Default.todo

GET         /report-your-payment-practices                           controllers.HomeController.index()

GET         /check-if-you-need-to-report                             controllers.QuestionnaireController.start()
GET         /check-if-you-need-to-report/exempt                      controllers.QuestionnaireController.exempt

GET         /check-if-you-need-to-report/company-or-llp              controllers.QuestionnaireController.companyOrLLP()
POST        /check-if-you-need-to-report/company-or-llp              controllers.QuestionnaireController.postCompanyOrLLP()

GET         /check-if-you-need-to-report/which-financial-year        controllers.QuestionnaireController.whichFinancialYear()
POST        /check-if-you-need-to-report/which-financial-year        controllers.QuestionnaireController.postWhichFinancialYear()

GET         /calculate-reporting-deadlines                           controllers.CalculatorController.show()
POST        /calculate-reporting-deadlines                           controllers.CalculatorController.submit()

